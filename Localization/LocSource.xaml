<?xml version="1.0"?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <!-- MainView -->

    <sys:String x:Key="LOCBc">BackgroundChanger</sys:String>
    
    <sys:String x:Key="LOCBcManageBackground">Manage background images</sys:String>

    <!-- BackgroundChangerSettingsView -->

    <sys:String x:Key="LOCBcEnableRandomSelect">Enable random selection</sys:String>
    <sys:String x:Key="LOCBcEnableAutoChanger">Enable automatic change background</sys:String>
    <sys:String x:Key="LOCBcTimeInterval">Time interval</sys:String>
    <sys:String x:Key="LOCBcSeconds">seconds</sys:String>

    <sys:String x:Key="LOCBcEnableImageAnimatedBackground">Enable animated background *</sys:String>
    <sys:String x:Key="LOCBcEnableImageAnimatedCover">Enable animated cover *</sys:String>

</ResourceDictionary>
